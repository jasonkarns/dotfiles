[core]
  excludesfile = ~/.gitignore
  editor = 'C:/Program Files/Notepad++/notepad++.exe' -multiInst -notabbar -nosession -noPlugin
  autocrlf = true
  safecrlf = warn
  hideDotFiles = dotGitOnly
[user]
  name = Jason Karns
  email = jason@karns.name
[github]
  user = jasonkarns
[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  glog = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  pom = push origin master
  diffstat = diff --stat
[color]
  ui = auto
  branch = auto
  diff = auto
  status = auto
  interactive = auto
  showbranch = auto
[color "branch"]
  current = reverse yellow
  local = yellow
  remote = green
#  plain =
[color "diff"]
  meta = bold yellow
  frag = bold magenta
  old = bold red
  new = bold green
#  plain = 
#  func = 
#  commit = 
  whitespace = reverse normal
[color "status"]
  header = normal
  added = yellow
  changed = green
  untracked = cyan
  nobranch = red
#[push]
#  default = matching
[branch "master"]
  remote = origin
  merge = refs/heads/master

[diff]
  tool = bc3
[difftool]
  prompt = false
[merge]
  tool = bc3

[difftool "bc3"]
  cmd = 'c:/program files/beyond compare 3/bcomp.exe' \"$LOCAL\" \"$REMOTE\"
[mergetool "bc3"]
  cmd = 'c:/program files/beyond compare 3/bcomp.exe' \"$LOCAL\" \"$REMOTE\" \"$BASE\" \"$MERGED\"
  trustExitCode = true

[difftool "winmerge"]
  cmd = 'c:/program files/winmerge/winmergeu.exe' \"$LOCAL\" \"$REMOTE\"
[mergetool "winmerge"]
  cmd = 'c:/program files/winmerge/winmergeu.exe' \"$LOCAL\" \"$REMOTE\" \"$MERGED\"
  trustExitCode = false
